{
    "manifest_version": 2,
    "name": "onion-everywhere",
    "version": "0.0.0",
    "description": "Automatically redirect to onion domain if it's available.",
    "homepage_url": "https://github.com/daurnimator/onion-everywhere",
    "applications": {
        "gecko": {
            "id": "onion-everywhere@daurnimator.com",
            "strict_min_version": "45.0"
        }
    },
    "background": {
        "scripts": [
            "node_modules/hi-base32/src/base32.js",
            "node_modules/js-sha3/src/sha3.js",
            "index.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
